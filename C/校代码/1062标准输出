#include<stdio.h>
int main()
{
	int cas,n,i;
	char a[1010][2000];
	scanf("%d",&cas);
	while(cas--)
	{
		printf("Case %d:\n",cas+1);
		scanf("%d",&n);
		for(i=1;i<=n;i++)
		gets(a[i]);
		for(i=1;i<=n;i++)
		{
			if(i%10==1)printf("%s",a[i]);
			else printf(" %s",a[i]);
			if(!i%10)printf("\n");
		}
		printf("\n");
	}
	return 0;
	
}

