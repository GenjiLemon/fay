#include<stdio.h>
int main()
{
	int a[10000],i,n,f=0,k=1,z;
	scanf("%d",&n);
	for(z=0;z<=(n-1);z++)
	{
		if(k==4)k=1;
		a[z]=k;k++;
	}
	while(f!=1)
	{f=0;
	for(i=0;i<=(n-1);i++)
		if(a[i]==3)a[i]=0;
	for(i=0;i<=(n-1);i++)
	{
		if(k==4)k=1;
		if(a[i]!=0)a[i]=k,k++,f++;
	}
	}
	for(i=0;i<=(n-1);i++)
		if(a[i]!=0) printf("%d",i);
	return 0;
		
}
